<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/content_information"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    app:layout_behavior="@string/appbar_scrolling_view_behavior"
    tools:context="com.guet.jason.sportsfriend.InformationActivity"
    tools:showIn="@layout/activity_information">

    <com.guet.jason.sportsfriend.MyView.X5WebView
        android:id="@+id/web_filechooser"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
    </com.guet.jason.sportsfriend.MyView.X5WebView>


    <View
        android:id="@+id/hiddenView"
        android:layout_width="match_parent"
        android:layout_height="30dp"
        android:layout_alignParentBottom="true"
        android:background="#fff" />
</RelativeLayout>
